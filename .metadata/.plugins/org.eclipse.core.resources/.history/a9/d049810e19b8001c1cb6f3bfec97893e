#include <stdio.h>
#include <stdlib.h>
#include "funcione.h"

int main(void) {
	setbuf(stdout,NULL);
	int km;
	int opcion;
	int latam;

	//int latamDos;
	//float latamTres;
	//float latamCuatro;
	int aerolineas;
	//int aerolineaDos;
	//float aerolineaTres;
	//float aerolineaCuatro;

	latam = 2343;


	printf("\n1- Ingresar Kilómetros. \n"
	        "2- Ingresar Precio de Vuelos. \n"
	        "3- Calcular todos los costos. \n"
	        "4- Informar Resultados. \n"
	        "5- Carga forzada de datos. \n"
	        "6- Salir. \n"
	        "\n Ingrese la opción deseada:");
	scanf("%d",&opcion);
	switch(opcion)
	{
	case 1:

		printf("cantidad de km\n");
		scanf("%d",&km);
		break;
	case 2:
		printf("precio de vuelo a latam:"
				"\nprecio de aerolinia: ");
		scanf("%d",&latam);
		scanf("%d",&aerolineas);

		break;
	case 3:

		precio(latam);


		break;
	}


	return latam;
}
